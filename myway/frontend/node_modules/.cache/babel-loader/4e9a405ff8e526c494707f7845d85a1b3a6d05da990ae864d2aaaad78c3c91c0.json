 {\r\n      cars: [],\r\n      searchQuery: ''\r\n    };\r\n  },\r\n  created() {\r\n    this.fetchCars();\r\n  },\r\n  methods: {\r\n    fetchCars() {\r\n      axios.get('http://localhost:8000/api/cars/')\r\n        .then(response => {\r\n          this.cars = response.data;\r\n        })\r\n        .catch(error => {\r\n          console.error('There was an error fetching the cars!', error);\r\n        });\r\n    },\r\n    searchCars() {\r\n      if (this.searchQuery.trim() === '') {\r\n        this.fetchCars();\r\n        return;\r\n      }\r\n      axios.get(`http://localhost:8000/api/cars/search?q=${this.searchQuery}`)\r\n        .then(response => {\r\n          this.cars = response.data;\r\n        })\r\n        .catch(error => {\r\n          console.error('There was an error searching for cars!', error);\r\n        });\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.catalogue {\r\n  text-align: center;\r\n  margin-top: 50px;\r\n}\r\n</style>"],"mappings":"AAmBA,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,SAAS,CAAC,CAAC;EAClB,CAAC;EACDC,OAAO,EAAE;IACPD,SAASA,CAAA,EAAG;MACVN,KAAK,CAACQ,GAAG,CAAC,iCAAiC,EACxCC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACP,IAAG,GAAIO,QAAQ,CAACR,IAAI;MAC3B,CAAC,EACAS,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,uCAAuC,EAAEA,KAAK,CAAC;MAC/D,CAAC,CAAC;IACN,CAAC;IACDE,UAAUA,CAAA,EAAG;MACX,IAAI,IAAI,CAACV,WAAW,CAACW,IAAI,CAAC,MAAM,EAAE,EAAE;QAClC,IAAI,CAACT,SAAS,CAAC,CAAC;QAChB;MACF;MACAN,KAAK,CAACQ,GAAG,CAAC,2CAA2C,IAAI,CAACJ,WAAW,EAAE,EACpEK,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACP,IAAG,GAAIO,QAAQ,CAACR,IAAI;MAC3B,CAAC,EACAS,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,wCAAwC,EAAEA,KAAK,CAAC;MAChE,CAAC,CAAC;IACN;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}